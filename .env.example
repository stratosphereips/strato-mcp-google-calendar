# Required
GOOGLE_CLIENT_ID=your_client_id
GOOGLE_CLIENT_SECRET=your_client_secret

# Optional
GOOGLE_REDIRECT_URI=http://localhost:8081
# TOKEN_STORE_PATH: defaults to /tokens in Docker, ~/.config/google-calendar-mcp/ locally.
# Uncomment and set only if you need a custom path.
# TOKEN_STORE_PATH=~/.config/google-calendar-mcp/
GOOGLE_SCOPES=https://www.googleapis.com/auth/calendar
DEFAULT_CALENDAR_ID=primary
LOG_LEVEL=WARNING
